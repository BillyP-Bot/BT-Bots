{
  "name": "billypbot",
  "description": "billypbot",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "postinstall": "npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . && prettier --check .",
    "format": "eslint --fix --ext .js,.jsx,.ts,.tsx . && prettier --write .",
    "watch": "tsc --watch",
    "start": "node .",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
  },
  "author": "Daniel Walsh",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "@distube/ytdl-core": "^4.11.8",
    "apisauce": "^3.0.0",
    "btbot-types": "^2.4.0",
    "discord.js": "^14.9.0",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "sodium-native": "^4.0.1"
  }
}
