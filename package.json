{
  "name": "billypbot",
  "description": "billypbot",
  "version": "1.0.0",
  "engines": {
    "node": "14.x",
    "npm": "*"
  },
  "main": "build/index.js",
  "scripts": {
    "db:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "db:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "db:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "db:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "build": "tsc",
    "postinstall": "npm run build",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "watch": "tsc --watch",
    "start": "node .",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
  },
  "author": "daniel walsh",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cron": "^1.7.2",
    "@types/get-image-colors": "^2.0.0",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7",
    "get-image-colors": "^4.0.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "typeorm": "^0.2.34",
    "winston": "^3.3.3",
    "ytdl-core": "^4.4.5",
    "ytdl-core-discord": "^1.2.5"
  }
}
